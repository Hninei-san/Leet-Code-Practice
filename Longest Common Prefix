class Solution:
    def longestCommonPrefix(self, strs: List[str]) -> str:
        common_str = strs[0]
        for i in range(1,len(strs)):
                j = 0
                while  j < len(common_str) and j < len(strs[i]) and common_str [j] == strs[i][j] :
                    j+= 1
                common_str = common_str[:j]
        return common_str
  
        
